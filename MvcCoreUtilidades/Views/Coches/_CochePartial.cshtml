@model List<Coche>

<h1>Vista Partial</h1>

<table class="table table-info">
    <thead>
        <tr>
            <th>ID</th>
            <th>MARCA</th>
            <th>MODELO</th>
            <th>IMAGEN</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Coche car in Model)
        {
            <tr>
                <td>@car.IdCoche</td>
                <td>@car.Marca</td>
                <td>@car.Modelo</td>
                <td>
                    <img src="@car.Imagen" style="width: 150px; height: 150px;" />
                </td>
            </tr>
        }
    </tbody>
</table>